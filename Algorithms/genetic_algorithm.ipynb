    "def initial_population(graph, population_size):\n",
    "    population = []\n",
    "    n = len(graph)\n",
    "    for _ in range(population_size):\n",
    "        genetic_code = [random.randint(0, 1) for _ in range(n)]\n",
    "        individual = Individual(genetic_code, graph)\n",
    "        population.append(individual)\n",
    "    return population"
   ],
